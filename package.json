    "test": "abc test",
    "test:watch": "abc test --watch -i",
    "finish": "npm run lint && npm test && npm run build"
  },
  "peerDependencies": {
    "swiper": "^5.2.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
